{
  "name": "waterwings-api",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "4.2.3"
  },
  "babel": {
      "presets": ["es2015", "stage-0"]
  },
  "scripts": {
    "db:migrate": "psql -h localhost -p 5432 -d quartermasterdb -U root -f ./migrations/index.sql",
    "db:seed": "psql -h localhost -p 5432 -d quartermasterdb -U root -f ./seeds.sql",
    "babel-node": "./node_modules/.bin/babel-node --ignore='node_modules'",
    "start": "NODE_ENV=production npm run babel-node -- ./app/index.js | ./node_modules/.bin/bunyan -o short",
    "dev": "./node_modules/.bin/nodemon --exec npm run babel-node -- ./app/index.js | ./node_modules/.bin/bunyan -o short",
    "test": "echo 'Currently no tests' && exit 0"
  },
  "author": "Rohan Nair",
  "dependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt": "^0.8.5",
    "bunyan": "^1.8.1",
    "co-multipart": "^2.0.0",
    "knex": "^0.9.0",
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-bouncer": "^5.0.0",
    "koa-cors": "0.0.16",
    "koa-jwt": "^1.1.1",
    "koa-router": "^5.3.0",
    "koa-unless": "0.0.1",
    "node-fetch": "^1.5.2",
    "node-uuid": "^1.4.7",
    "objection": "^0.4.0-rc.3",
    "pg": "^4.4.3",
    "ramda": "^0.21.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-tape-runner": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.11.3",
    "nodemon": "^1.8.1",
    "path": "^0.12.7",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  }
}
